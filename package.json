    "@types/jasmine": "^4.0.3",
    "@types/leaflet": "^1.7.10",
    "@types/mime": "^2.0.3",
    "@types/node": "^17.0.40",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~4.1.1",
    "karma": "^6.3.20",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
