{
  "name": "application-client",
  "version": "1.0.0",
  "license": "MIT",
  "prettier": {
    "tabWidth": 2,
    "printWidth": 200
  },
  "contributors": [],
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ssr": "node dist/server",
    "build": "ng build",
    "build:client": "ng build --configuration production",
    "build:server": "ng run application-client:server:production",
    "build:stats": "ng build --stats-json --configuration production",
    "build:contract": "node ./contracts/build.js",
    "build:contract:ropsten": "node ./contracts_ropsten/build.js",
    "build:staging": "ng run application-client:staging:production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:build": "npm run build:client && npm run build:server",
    "start:prod": "pm2 start dist/server --name 'application-client'",
    "stop:prod": "pm2 stop --name 'application-client'",
    "analyze": "webpack-bundle-analyzer dist/application-client/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "^14.0.0",
    "@angular/animations": "^14.0.0",
